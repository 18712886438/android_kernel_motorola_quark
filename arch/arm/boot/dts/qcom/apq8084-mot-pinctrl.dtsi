		/* AP2MDM_STATUS, AP2MDM_ERRFATAL, AP2MDM_SOFTRESET */
		ap2mdm {
			qcom,pins = <&gp 110 &gp 112 &gp 128>;
			qcom,num-grp-pins = <3>;
			qcom,pin-func = <0>;
			label = "ap2mdm";
			ap2mdm_default: default {
				bias-disable;
				drive-strength = <8>;
			};
		};

		/* MDM2AP_STATUS, MDM2AP_ERRFATAL */
		mdm2ap {
			qcom,pins = <&gp 109 &gp 111>;
			qcom,num-grp-pins = <2>;
			qcom,pin-func = <0>;
			label = "mdm2ap";
			mdm2ap_default: default {
				bias-pull-down;
				drive-strength = <8>;
			};
		};

		/* MDM2AP_PBL_READY */
		mdm2ap_pbl {
			qcom,pins = <&gp 113>;
			qcom,num-grp-pins = <1>;
			qcom,pin-func = <0>;
			label = "mdm2ap_pbl";
			mdm2ap_pbl_default: default {
				bias-disable;
				drive-strength = <8>;
			};
		};

		headset_det {
			qcom,pins = <&gp 9>;
			qcom,num-grp-pins = <1>;
			qcom,pin-func = <0>;
			label = "headset_det";
			headset_det_default: default {
