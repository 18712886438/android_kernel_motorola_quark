			qcom,pin-func = <0>;
			label = "nfc_irq";
			nfc_irq_default: default {
				bias-pull-down;
				drive-strength = <2>;
			};
		};

		nfc_ven {
			qcom,pins = <&gp 116>;
			qcom,num-grp-pins = <1>;
			qcom,pin-func = <0>;
			label = "nfc_ven";
			nfc_ven_default: default {
				bias-pull-down;
				output-low;
				drive-strength = <2>;
			};
		};

		msm_pcie {
