		/* blsp uart configuration */
		blsp_uart  {
			qcom,pins = <&gp 55 &gp 56 &gp 57 &gp 58>;
			qcom,num-grp-pins = <4>;
			qcom,pin-func = <2>;
			label = "blsp_uart";
			blsp_uart_default: default {
				bias-disable;
				drive-strength = <16>;
			};
		};

		/* blsp uart configuration */
		blsp_uart_weak  {
			qcom,pins = <&gp 51 &gp 52>;
			qcom,num-grp-pins = <2>;
