				drive-strength = <2>;
			};
			hsic_wake_active: active {
				bias-pull-down;
				drive-strength = <8>;
			};
		};

		/* high speed interface qcom,xhci-msm-hsic */
		/* AP2MDM_WAKEUP		           */
		hsic_resume  {
			qcom,pins = <&gp 108>;
			qcom,num-grp-pins = <1>;
			qcom,pin-func = <0>;
			label = "hsic_resume";
			hsic_resume_default: default {
				bias-disable;
				drive-strength = <2>;
			};
			hsic_resume_active: active {
				bias-pull-down;
				output-low;
				drive-strength = <8>;
