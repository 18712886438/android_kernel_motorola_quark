				drive-strength = <2>;
			};
		};

		stm401_in {
			qcom,pins = <&gp 1 &gp 122 &gp 141>;
			qcom,num-grp-pins = <3>;
			qcom,pin-func = <0>;
			label = "stm401_in";
			stm401_in_default: default {
				bias-disable;
				drive-strength = <2>;
			};
		};

		blsp1_uart6 {
			qcom,pins = <&gp 43 &gp 44 &gp 45 &gp 46>;
			qcom,num-grp-pins = <4>;
			qcom,pin-func = <2>;
			label = "blsp1_uart6";
			blsp1_uart6_default: default {
				bias-disable;
				drive-strength = <16>;
			};
		};

		/*  used by ti,c55-ctrl */
		c55_ap_int  {
