&soc {
	tlmm_pinmux: pinctrl@fd510000 {

		pinctrl-names = "default";
		pinctrl-0 =	 <
					&ap2mdm_default
					&mdm2ap_default
					&mdm2ap_pbl_default
					&headset_det_default
					&tmp108_irq_default
					&cycapsense_default
					&cycapsense_data_default
					&wlan_enable_default
					&nfc_irq_default
					&nfc_ven_default
					&msm_pcie_default
					&stm401_in_default
					&stm401_out_default
					&blsp1_uart6_default
					&c55_ap_int_default
					&c55_c55_int_default
